            </zeroOrMore>
            <zeroOrMore>
               <ref name="exemplum"/>
            </zeroOrMore>
            <optional>
               <ref name="remarks"/>
            </optional>
            <zeroOrMore>
               <ref name="listRef"/>
            </zeroOrMore>
         </group>
         <ref name="att.global.attributes"/>
         <ref name="att.identified.attributes"/>
         <ref name="att.typed.attributes"/>
         <empty/>
      </element>
